CREATE TABLE IF NOT EXISTS students (
	school varchar(2) NOT NULL,
	sex varchar(1) NOT NULL,
	age int NOT NULL,
	address varchar(1) DEFAULT NULL,
	famsize varchar(3) DEFAULT NULL,
	Pstatus varchar(1) DEFAULT NULL,
	Medu int DEFAULT NULL,
	Fedu int DEFAULT NULL,
    Mjob text DEFAULT NULL,
    Fjob text DEFAULT NULL,
    reason text DEFAULT NULL,
    guardian text DEFAULT NULL,
    traveltime int DEFAULT NULL,
    studytime int DEFAULT NULL,
    failures int DEFAULT NULL,
    schoolsup text DEFAULT NULL,
    famsup text DEFAULT NULL,
    paid text DEFAULT NULL,
    activities text DEFAULT NULL,
    nursery text DEFAULT NULL,
    higher text DEFAULT NULL,
    internet text DEFAULT NULL,
    romantic text DEFAULT NULL,
    famrel int DEFAULT NULL,
    freetime int DEFAULT NULL,
    goout int DEFAULT NULL,
    Dalc int DEFAULT NULL,
    Walc int DEFAULT NULL,
    health int DEFAULT NULL,
    absences int DEFAULT NULL,
    G1 int DEFAULT NULL,
    G2 int DEFAULT NULL,
    G3 int DEFAULT NULL
);

COPY students FROM '/home/nataliabernardo/flask/df_students.csv' DELIMITER ',' CSV HEADER;